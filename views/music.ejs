<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel='stylesheet' href='/stylesheets/bootstrap.css' />
  </head>
  <body>
    <%- include header %>
    <header class="main-header" style="background-image: url(images/1492673792372face02.jpg)" "="">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">

                    <!-- start logo -->
                    <h3>万千歌手 近在眼前</h3>
                    <!-- end logo -->

                </div>
            </div>
        </div>
    </header>
    <div class="row" id="box">
      <% list.forEach(function(item,idx){ %>
        <div class="col-sm-4 col-md-3 col-xs-6">
          <div class="thumbnail">
            <img class="lazy" width="200" height="200" src="/images/upload/load.jpg" data-original="<%- item.pic_big %>" id="<%- item.artist_id %>">
            <div class="caption top">
              <a href=""><%- item.title %></a>
            </div>
          </div>
        </div>
     <%})%>
    </div>
    <%- include footer %>
    <script src="/javascripts/jquery-1.11.3.js"></script>
    <script src="/javascripts/jquery.lazyload.js"></script>
    <script src="/javascripts/bootstrap.js"></script>
    <script type="text/javascript" charset="utf-8">
      $(function() {
          $("img.lazy").lazyload({effect: "fadeIn",container:$('#box')});
      }); 
    </script>
  </body>
</html>